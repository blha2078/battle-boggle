<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.battleboggle.GameboardController">
   <children>
      <Label layoutX="65.0" layoutY="289.0" text="Enter a word">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField layoutX="65.0" layoutY="329.0" prefHeight="26.0" prefWidth="244.0" />
      <Text layoutX="458.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Time:">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <Button fx:id="startButton" layoutX="509.0" layoutY="323.0" mnemonicParsing="false" onAction="#startGameButton" text="Start">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Label fx:id="countdownLabel" layoutX="523.0" layoutY="38.0" text="3:00">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
